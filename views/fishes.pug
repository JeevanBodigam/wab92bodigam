extends layout

block content
  h1= title
  - let fishes=[{fishname:"Apollo Fish",habitat:"Lowland Rivers",classification:"Cyprinidae"},{fishname:"Paroon Shark",habitat:"River Channels",classification:"Pangasiidae"},{fishname:"Brichardi",habitat:"Rocky Substrates",classification:"Cichlidae"}]
  div(class="container-fluid")
  div(class="row")
    each fish in fishes
        div(class="col-sm-4") #{fish.fishname} - #{fish.habitat} - #{fish.classification}

  